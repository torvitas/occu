### rfd-config##source common.tclproc main { } {	set BUFFER_SIZE [expr 32 * 1024]	set sid  [getQueryArg sid]	set file [getQueryArg file]		if { [session_isValid $sid] == "false" } { error INVALID_SESSION }		set fd [open $file r]	catch {fconfigure $fd -translation binary}	catch {fconfigure $fd -encoding binary}	puts "Content-Type: text/plain"	puts ""	catch {fconfigure stdout -translation binary}	catch {fconfigure stdout -encoding binary}	while { ! [eof $fd]} {			puts -nonewline [read $fd $BUFFER_SIZE]	}	close $fd	}startup