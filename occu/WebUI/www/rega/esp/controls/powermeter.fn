                      WriteLine("<td>");
                        WriteLine("<span>${lblInfoStatusControlEnergyCounter}</span>");
                      WriteLine("</td>");
                    WriteLine("</tr>");
                    WriteLine("<tr>");
                      WriteLine("<td>");
