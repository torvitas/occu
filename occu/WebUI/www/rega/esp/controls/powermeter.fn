                      WriteLine("</tr>");
                      WriteLine("<tr>");
                        WriteLine("<td>");
                        WriteLine("<span>"#oEnergyCounter.Value().ToString(2)#" "#oEnergyCounter.ValueUnit()#"</span><br/><span id='"#chnId#"EnergyCostDevice' class='hidden'></span>");
