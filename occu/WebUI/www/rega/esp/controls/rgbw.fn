              WriteLine("<input id='infoSliderPos"#chnId#"' type='text' value='0' size='3' class='CLASS02542' style='margin-top: 6px; text-align:center' disabled/>&nbsp;${lblKelvin}");
            WriteLine("</div>");
          WriteLine("</td>");
        WriteLine("</tr>");
        WriteLine("<tr>");
          WriteLine("<td colspan='2'>");
            WriteLine("<div id='slider"#chnId#"' class='ControlBtnInfo' style='width:220px;'></div>");
          WriteLine("</td>");
        WriteLine("</tr>");
      WriteLine("</table>");
    WriteLine("</td>");
  WriteLine("</tr>");

  WriteLine("<script type='text/javascript'>");
    WriteLine("var opts = {};");
    WriteLine("opts.Type = 'WHITE';");
    WriteLine("opts.whiteLevelID = '"#whiteLevelID#"';");
    WriteLine("opts.whiteLevelValue = '"#whiteLevelValue#"';");
